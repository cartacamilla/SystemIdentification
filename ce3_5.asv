clear all
close all
clc

flexibleData = load('CE.mat');

%% Initialisation
u = flexibleData.u;
y = flexibleData.y;

[N,M] = size(y);

Te = 0.015;

%% Create Data Object

data = iddata(y,u,Te);

[data_d,T] = detrend(data);

plot(data,data_d)


%% Order estimation
nc = 0; nd = 0; nf = 0; nk = 1;

for n =1:10
    orders = [n n nk];
    SYS = arx(data_d, orders);
    stem(n,SYS.Report.Fit.LossFcn), hold on
end






